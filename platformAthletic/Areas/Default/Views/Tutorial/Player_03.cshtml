<div class="modal fade " id="modalTutorial" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content tutorial-wrapper">
            <div class="modal-body">
                <div class="breadcrumb"><strong> <span class="nextBtn" data-step="1">Welcome</span> / Info </strong> / My Page  / Wrap-Up</div>

                <h1>Tell us about yourself!</h1>
                <p>Fill in all the fields that apply. We use this information to make sure you get on the right leaderboards.</p>
                <div id="AddPlayerInfoWrapper">

                    @Html.Action("AddPlayerUserInfo", "User")
                </div>
            </div>
            <div class="modal-footer text-center">
                <button type="button" class="btn btn-primary" id="SubmitAddPlayerInfoBtn">Submit</button>
            </div>
        </div>
    </div>
</div>
