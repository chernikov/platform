@model platformAthletic.Model.User
@{
    ViewBag.Title = "Choose your team colors";
    Layout = "~/Areas/Default/Views/Shared/_Layout.cshtml";
}
@section header 
{
    <script type="text/javascript" src="/Media/scripts/default/jquery.ImageColorPicker.js"></script>
    <script type="text/javascript" src="/Media/scripts/default/set-color.js"></script>
}
@if (Model.InRoles("coach")) {
    @Html.Partial("SetColorsTeam", Model)
} else {
    @Html.Partial("SetColorsIndividual", Model)
}
    